<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css" >
    <title>Document</title>
    
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Calistoga&family=Goblin+One&family=Luckiest+Guy&family=Righteous&family=Sintony:wght@400;700&display=swap" rel="stylesheet">

</head>
<body>
    <div class="navbar">
        <div class="title">
            <img src="images/logo.jpg">
            <p>FastCards</p>
        </div>
        <div class="logout">
            <a href="/logout">
                <button class="logout">logout</button>
            </a>
        </div>
    </div>
    <div class="createfolder">
        <div class="innercreatefolder">
            <p>score: <%= score %></p>
            <% for (let i = 0; i < questions.length; i++) { %>
                <p>Question: <%= questions[i] %></p>
                <p>Answer: <%= answers[i] %></p>
                <p>Result: <%= checking[i] %></p>
            <% } %>
            
            <a href="/score">
                <button class="logout">done</button>
            </a>
        </div>
    </div>
</body>
</html>